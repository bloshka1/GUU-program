#include <stdio.h>

int main() {
    double x, y;
    printf("Введите координаты точки x и y: ");
    scanf("%lf %lf", &x, &y);

    // Проверка: первая или третья четверть?
    if ((x > 0 && y > 0) || (x < 0 && y < 0)) {
        printf("True\n");
    } else {
        printf("False\n");
    }

    return 0;
}
