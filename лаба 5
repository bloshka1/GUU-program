#include <stdio.h>

int f(int n) {
    // Массивы: количество букв для чисел 0–19 (0 не используется, но для удобства)
    int units[20] = {
        0, 4, 3, 3, 6, 4, 5, 4, 6, 6,  // 0-9
        6, 11, 10, 10, 12, 10, 11, 10, 12, 12  // 10-19
    };

    int tens[10] = {
        0, 0, 8, 8, 5, 9, 10, 9, 11, 10  // 0,1 не используются; 2→"двадцать"=8, ..., 9→"девяносто"=10
    };

    if (n >= 1 && n <= 19) {
        return units[n];
    } else if (n >= 20 && n <= 99) {
        int t = n / 10;   // десятки
        int u = n % 10;   // единицы
        return tens[t] + (u > 0 ? units[u] : 0);
    }
    return 0; // не должно происходить при корректном вводе
}

int main() {
    int n;
    printf("Введите натуральное число n (1 <= n < 100): ");
    scanf("%d", &n);

    if (n < 1 || n >= 100) {
        printf("Ошибка: n должно быть от 1 до 99.\n");
        return 1;
    }

    int result = f(n);
    printf("f(%d) = %d\n", n, result);
    return 0;
}
