#include <stdio.h>

int main() {
    int K;
    printf("Введите номер дня в году (1-365): ");
    scanf("%d", &K);

    // Проверка корректности ввода (опционально)
    if (K < 1 || K > 365) {
        printf("Ошибка: K должно быть в диапазоне от 1 до 365.\n");
        return 1;
    }

    // Вычисление дня недели
    int day = (K + 5) % 7;  // потому что 1 января — суббота (6)
    if (day == 0) {
        day = 7;
    }

    printf("Номер дня недели: %d\n", day);
    return 0;
}
