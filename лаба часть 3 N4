#include <stdio.h>
#include <string.h>

// Процедура OneChar(S, S0)
// S — входная строка (const, не модифицируется)
// S0 — выходная строка (должна быть достаточно велика)
void OneChar(const char S[], char S0[]) {
    // Массив для подсчёта частоты символов (предполагаем ASCII)
    int freq[256] = {0};  // инициализируем нулями

    // 1. Подсчитываем частоту каждого символа
    for (int i = 0; S[i] != '\0'; i++) {
        unsigned char c = S[i];  // чтобы избежать отрицательных индексов
        freq[c]++;
    }

    // 2. Формируем S0: добавляем символы с частотой 1 в порядке появления
    int j = 0;
    for (int i = 0; S[i] != '\0'; i++) {
        unsigned char c = S[i];
        if (freq[c] == 1) {
            S0[j++] = S[i];
        }
    }
    S0[j] = '\0';  // завершаем строку
}

// Основная программа для тестирования
int main() {
    char S[1000];
    char S0[1000];

    printf("Введите строку S: ");
    fgets(S, sizeof(S), stdin);

    // Удалим символ новой строки, если есть
    size_t len = strlen(S);
    if (len > 0 && S[len - 1] == '\n') {
        S[len - 1] = '\0';
    }

    OneChar(S, S0);

    printf("Символы, встречающиеся один раз: \"%s\"\n", S0);
    return 0;
}
